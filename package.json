{
  "name": "xcui",
  "version": "2.0.0-rc-7",
  "main": "dist/xcui.js",
  "engines": {
    "node": ">=5.10.0"
  },
  "description": "Desktop web UI based on Vue and Bootstrap",
  "theme": "./custom.js",
  "keywords": [
    "xcui",
    "vue",
    "vue-components",
    "web-components",
    "ui-components",
    "component",
    "components",
    "desktop ui",
    "framework",
    "frontend"
  ],
  "author": "wmfe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfe/xcui"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "site": "rimraf site && cross-env NODE_ENV=production webpack -p --progress --hide-modules --colors --config build/webpack.site.conf.js",
    "site:local": "rimraf site && webpack -p --progress --hide-modules --colors --config build/webpack.site.conf.js",
    "build": "rimraf dist/* && npm run build:umd && npm run build:esmodule",
    "build:esmodule": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config build/webpack.npm.conf.js",
    "build:umd": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config build/webpack.npm.umd.conf.js",
    "build:components": "node build/build-components"
  },
  "dependencies": {
    "async-validator": "^1.6.9",
    "popper.js": "^1.0.7",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-transform-vue-jsx": "^3.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "cli-spinner": "^0.2.4",
    "co": "^4.6.0",
    "co-fs": "^1.2.0",
    "co-request": "^1.0.0",
    "connect-history-api-fallback": "^1.1.0",
    "copy-webpack-plugin": "^1.1.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.5.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-html": "^1.4.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.8.4",
    "function-bind": "^1.0.2",
    "html-webpack-plugin": "^2.14.0",
    "inject-loader": "^2.0.1",
    "json-loader": "^0.5.4",
    "jsonp": "^0.2.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "less-vars-to-js": "^1.1.2",
    "markdown-it-container": "^2.0.0",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^2.0.8",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.2.0",
    "vue-loader": "^9.9.5",
    "vue-router": "^2.0.0",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler-loader": "^1.0.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "xcui-demo-loader": "^0.1.8"
  }
}
